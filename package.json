{
  "name": "@factiverse/design-system",
  "description": "Factiverse Design System is a reusable component library that helps Factiverse contributors build UIs faster. The goal is to make building durable UIs more productive and satisfying.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/factiverse/storybook-design-system.git"
  },
  "author": "Tobias Tykvart",
  "license": "UNLICENSED",
  "version": "0.1.7",
  "private": false,
  "bugs": {
    "url": "https://github.com/factiverse/storybook-design-system/issues"
  },
  "homepage": "https://github.com/factiverse/storybook-design-system#readme",
  "dependencies": {
    "polished": "^4.1.1",
    "styled-components": "^5.2.1",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "rm -rf ./dist && cross-env BABEL_ENV=production babel src -d dist",
    "test": "jest && eslint .",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "build-storybook-docs": "build-storybook  --docs",
    "chromatic": "npx chromatic --project-token=e8130cbb59b8 --exit-once-uploaded",
    "deploy-storybook": "storybook-to-ghpages",
    "release": "auto shipit --base-branch=main"
  },
  "babel": {
    "presets": [
      [
        "react-app",
        {
          "absoluteRuntime": false
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "peerDependencies": {
    "react": ">=17.0.1",
    "react-dom": ">=17.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.4.1",
    "@mui/material": "^5.4.1",
    "@storybook/addon-a11y": "^6.4.18",
    "@storybook/addon-actions": "^6.4.18",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-interactions": "^6.4.18",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/node-logger": "^6.4.18",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.4.18",
    "@storybook/storybook-deployer": "^2.8.10",
    "@storybook/testing-library": "^0.0.9",
    "@types/react-dom": "^17.0.11",
    "auto": "^10.32.6",
    "chromatic": "^6.4.3",
    "cross-env": "^7.0.3",
    "prettier": "^2.5.1",
    "react-docgen-typescript-plugin": "^1.0.0",
    "react-dom": "^17.0.2",
    "typescript": "^4.5.5",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "react": "^17.0.1",
    "react-scripts": "4.0.3"
  }
}
